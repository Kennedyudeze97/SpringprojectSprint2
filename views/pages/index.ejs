<!DOCTYPE html>
<html lang="en">
    <head>

    </head>
    <body class="container">
        <main>
            <div class="jumbotron">
                <h1>Top Mascots</h1>
                <p><%= tagline %></p>
                <ul>
                    <% mascots.forEach(function(mascot) { %>
                        <li>
                            <strong><%= mascot.name %></strong>
                            representing <%= mascot.organization %>
                        </li>
                    <% }); %>
                </ul>
            </div>
        </main>
    </body>
</html>